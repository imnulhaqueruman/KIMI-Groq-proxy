{
  "name": "kimi-groq-proxy",
  "version": "0.1.0",
  "description": "Bun-based proxy server that converts Anthropic tool messages to Groq/Kimi-K2 format",
  "main": "src/proxy.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch run src/proxy.ts",
    "start": "bun run src/proxy.ts",
    "build": "bun build src/proxy.ts --outdir ./dist --target node",
    "build:worker": "wrangler deploy --dry-run --outdir=dist",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "deploy": "wrangler deploy",
    "dev:worker": "wrangler dev"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "openai": "^4.28.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.0.0"
  },
  "keywords": [
    "anthropic",
    "groq",
    "kimi",
    "proxy",
    "bun",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}